#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = EditAppMDI.exe EditAppSDI.exe EditAppWorkbook.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

EditAppMDI.exe: EditAppMDI.dpr
  $(DCC)

EditAppSDI.exe: EditAppSDI.dpr
  $(DCC)

EditAppWorkbook.exe: EditAppWorkbook.dpr
  $(DCC)
