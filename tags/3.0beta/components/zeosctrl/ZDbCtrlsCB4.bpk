PROJECT = ZDbCtrlsCB4.bpl
OBJFILES = dbctrls\ZImage.obj dbctrls\ZDbCtrlsReg.obj dbctrls\ZFilterDlg.obj \
  dbctrls\ZFindDlg.obj dbctrls\ZGrid.obj dbctrls\ZDbCtrlsConst.obj ZDbCtrlsCB4.obj
RESFILES = dbctrls\ZImage.dcr dbctrls\ZFilterDlg.dcr dbctrls\ZFindDlg.dcr dbctrls\ZGrid.dcr
RESDEPEN = $(RESFILES) dbctrls\ZFilterDlg.dfm dbctrls\ZFindDlg.dfm
LIBFILES =
