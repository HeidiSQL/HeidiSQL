PACKAGES = vcl40.bpi vcldb40.bpi vcljpg40.bpi ZCommonCB4.bpi ZDbwareCB4.bpi ZUtilsCB4.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;dbctrls
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
USERDEFINES =
SYSDEFINES = _RTLDLL;NO_STRICT;USEPACKAGES
# ---------------------------------------------------------------------------
CFLAG1 = -Idbctrls;utils;resources;$(BCB)\include;$(BCB)\include\vcl -Od -Hc \
  -H=$(BCB)\lib\vcl40.csm -w -Ve -r- -a8 -k -y -v -vi- -c -b- -w-par -w-inl -Vx \
  -tWM -D$(SYSDEFINES);$(USERDEFINES)
PFLAGS = -U..\zeosdbo;dbctrls;utils;resources;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -Idbctrls;utils;resources;$(BCB)\include;$(BCB)\include\vcl -$YD -$W -v -JPHNE -M
RFLAGS = -idbctrls;utils;resources;$(BCB)\include;$(BCB)\include\vcl
AFLAGS = /idbctrls /iutils /iresources /i$(BCB)\include /i$(BCB)\include\vcl /mx /w2 /zd
LFLAGS = -L..\zeosdbo;dbctrls;utils;resources;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -D"Zeos Dbware Controls" -aa -Tpp -x -Gn -Gl -Gi -v
# ---------------------------------------------------------------------------
ALLOBJ = c0pkg32.obj Memmgr.Lib $(PACKAGES) sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mti.lib
